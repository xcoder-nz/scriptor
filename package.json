{
  "name": "scriptor",
  "version": "1.0.0",
  "nyc": {
    "all": true,
    "extension": [
      ".ts"
    ],
    "include": [
      "cli/**/*.ts",
      "cli/prompts/**/*.ts"
    ],
    "exclude": [
      "tests/",
      "dist/"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "source-map": true,
    "instrument": true
  },
  "main": "index.js",
  "scripts": {
    "test": "jest --config ./jest.config.js",
    "test:coverage": "jest --config ./jest.config.js --coverage",
    "test:unit": "jest --config ./jest.config.js --coverage=false --testPathPattern=tests/unit",
    "test:int": "jest --config ./jest.config.js --coverage=false --testPathPattern=tests/integration",
    "test:int:cov": "nyc --silent npm run test:int",
    "test:file": "jest --config ./jest.config.js --",
    "test:grep": "jest --config ./jest.config.js -t",
    "lint": "eslint --ext .ts .",
    "typecheck": "tsc --noEmit",
    "check": "npm run lint && npm run typecheck && npm run test"
  },
  "keywords": [],
  "author": "Carlos Pinto xcoder-nz",
  "license": "MIT",
  "description": "",
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.1",
    "@types/yargs": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "@typescript-eslint/parser": "^8.30.1",
    "@vercel/ncc": "^0.38.3",
    "dotenv": "^16.5.0",
    "eslint": "^9.24.0",
    "eslint-plugin-jest": "^28.11.0",
    "jest": "^29.7.0",
    "nyc": "^17.1.0",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "yargs": "^17.7.2"
  },
  "dependencies": {
    "nock": "^14.0.3",
    "openai": "^4.95.0"
  }
}
